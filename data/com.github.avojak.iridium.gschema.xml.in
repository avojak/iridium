<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="@schema-path@" id="@appid@">
    <key name="prefer-dark-style" type="b">
      <default>false</default>
      <summary>If the dark Gtk stylesheet should be used</summary>
      <description>If the dark Gtk stylesheet should be used</description>
    </key>
    <key name="certificate-validation-policy" type="s">
      <default>"REJECT"</default>
      <summary>How to handle unacceptable SSL/TLS certificates</summary>
      <description>How to handle unacceptable SSL/TLS certificates</description>
    </key>
    <key name="default-nickname" type="s">
      <default>""</default>
      <summary>The default nickname when connecting to a server</summary>
      <description>The default nickname when connecting to a server</description>
    </key>
    <key name="default-realname" type="s">
      <default>""</default>
      <summary>The default real name when connecting to a server</summary>
      <description>The default real name when connecting to a server</description>
    </key>
    <key name="suppress-connection-close-warnings" type="b">
      <default>false</default>
      <summary>If warnings should be suppressed when closing a server connection</summary>
      <description>If warnings should be suppressed when closing a server connection</description>
    </key>
    <key name="remember-connections" type="b">
      <default>true</default>
      <summary>If server connections should be remembered between sessions</summary>
      <description>If server connections should be remembered between sessions</description>
    </key>
  </schema>
</schemalist>
